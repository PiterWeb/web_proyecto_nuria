---
import Container from "@components/container.astro";
import Cta from "@components/cta.astro";
import Noticias from "@components/custom/news/noticias.astro";
import Hero from "@components/hero.astro";
import Layout from "@layouts/Layout.astro";
import Videos from "@components/custom/video/videos.astro";
---


<Layout title="">
  <Container>
    <Hero />
    <Noticias />
    <Videos />
    <Cta />
  </Container>
</Layout>

<style is:global>
  html {
    scroll-behavior: smooth;
  }
</style>
